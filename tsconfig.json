{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "dist",
      "rootDir": "src",
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@/*":["src/*"]
      },
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules", ".vscode","src/**/*Test.ts"
    ]
  }
  